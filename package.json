{
  "name": "cash-dnr-backend",
  "version": "1.0.0",
  "description": "Cash Digital Note Registry Backend - Phase 1 MVP",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "set NODE_ENV=development&& nodemon src/server.js",
    "dev:prod": "set NODE_ENV=production&& nodemon src/server.js",
    "start:dev": "set NODE_ENV=development&& node src/server.js",
    "start:prod": "set NODE_ENV=production&& node src/server.js",
    "migrate": "node src/utils/databaseSetup.js",
    "migrate:phase1": "node -e \"import('./src/migrations/20241217_002_phase1_foundational_operations.js').then(m => m.up()).then(() => console.log('✅ Phase 1 migration completed!')).catch(console.error)\"",
    "migrate:rollback": "node -e \"import('./src/migrations/20241217_002_phase1_foundational_operations.js').then(m => m.down()).then(() => console.log('✅ Migration rolled back!')).catch(console.error)\"",
    "db:setup": "node src/utils/databaseSetup.js",
    "db:seed": "node src/utils/seedDatabase.js",
    "test": "jest",
    "lint": "eslint src/",
    "build": "vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "cash",
    "registry",
    "tax",
    "tracking",
    "south-africa"
  ],
  "author": "TOBUN",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.5.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "vite": "^4.4.9"
  }
}
